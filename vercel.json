{
  "version": 2,
  "builds": [
    {
      "src": "ray_cheng_frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "ray_cheng_frontend/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/ray_cheng_frontend/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/ray_cheng_frontend/index.html"
    }
  ],
  "buildCommand": "cd ray_cheng_frontend && npm run build",
  "outputDirectory": "ray_cheng_frontend/build"
}
