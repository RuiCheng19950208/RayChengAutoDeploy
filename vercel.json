{
  "version": 2,
  "builds": [
    {
      "src": "ray_cheng_frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd ray_cheng_frontend && npm install && npm run build",
  "outputDirectory": "ray_cheng_frontend/build"
}
